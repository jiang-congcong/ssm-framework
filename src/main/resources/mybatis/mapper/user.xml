<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="cn.ccj.dao.UserDAO">
    <select id="all" resultType="cn.ccj.entity.User">
        select
        id,
        username,
        password,
        phone
        from
        user
    </select>

</mapper>